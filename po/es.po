msgid "Usage:\n"
msgstr "Modo de empleo:\n"

#: ../src/gpaste.vala:51
#, fuzzy, c-format
msgid "%s [history]: print the history with indexes\n"
msgstr "%s [history]: imprimir el historial\n"

#: ../src/gpaste.vala:52
#, fuzzy, c-format
msgid "%s raw-history: print the history without indexes\n"
msgstr "%s raw-history: imprimir el historial\n"

#: ../src/gpaste.vala:53
#, c-format
msgid "%s add <text>: set text to clipboard\n"
msgstr "%s add <texto>: enviar <texto> al portapapeles\n"

#: ../src/gpaste.vala:54
#, c-format
msgid "%s get <number>: get the <number>th item from the history\n"
msgstr ""
"%s get <número>: obtener el elemento número <número> del historial del "
"portapapeles\n"

#: ../src/gpaste.vala:55
#, c-format
msgid ""
"%s set <number>: set the <number>th item from the history to the clipboard\n"
msgstr ""
"%s set <número>: enviar el elemento número <número> del historial al "
"portapapeles\n"

#: ../src/gpaste.vala:56
#, c-format
msgid "%s delete <number>: delete <number>th item of the history\n"
msgstr ""
"%s delete <número>: suprimir el elemento número <número> del historial\n"

#: ../src/gpaste.vala:57
#, c-format
msgid ""
"%s file <path>: put the content of the file at <path> into the clipboard\n"
msgstr ""
"%s file <ruta>: colocar el contenido del fichero indicado por <ruta>en el "
"portapapeles\n"

#: ../src/gpaste.vala:58
#, c-format
msgid "whatever | %s: set the output of whatever to clipboard\n"
msgstr "loquesea | %s: enviar la salida de <loquesea> al portapapeles\n"

#: ../src/gpaste.vala:59
#, c-format
msgid "%s empty: empty the history\n"
msgstr "%s empty: vaciar el historial\n"

#: ../src/gpaste.vala:60
#, c-format
msgid "%s start: start tracking clipboard changes\n"
msgstr "%s start: iniciar seguimiento de cambios en el portapapeles\n"

#: ../src/gpaste.vala:61
#, c-format
msgid "%s stop: stop tracking clipboard changes\n"
msgstr "%s stop: detener seguimiento de cambios en el portapapeles\n"

#: ../src/gpaste.vala:62
#, c-format
msgid "%s quit: alias for stop\n"
msgstr "%s quit: alias de stop\n"

#: ../src/gpaste.vala:63
#, c-format
msgid "%s daemon-reexec: reexecute the daemon (after upgrading...)\n"
msgstr ""
"%s daemon-reexec: vuelva a ejecutar el servicio (después de actualizar...)\n"

#: ../src/gpaste.vala:64
#, c-format
msgid "%s settings: launch the configuration tool\n"
msgstr "%s settings: iniciar la herramienta de configuración\n"

#: ../src/gpaste.vala:65
